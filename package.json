{
  "name": "polygon",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "doppler run -- next build",
    "build:test": "next build",
    "build:prod": "next build",
    "check": "doppler run -- next lint && tsc --noEmit",
    "db:generate": "doppler run -- prisma migrate dev",
    "db:migrate": "doppler run -- prisma migrate deploy",
    "db:push": "doppler run -- prisma db push",
    "db:studio": "doppler run -- prisma studio",
    "dev": "doppler run node server.js",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "doppler run -- next lint",
    "lint:fix": "doppler run -- next lint --fix",
    "postinstall": "prisma generate",
    "preview": "doppler run -- next build && doppler run -- next start",
    "start": "doppler run -- sh -c 'prisma migrate deploy && NODE_ENV=production node server.js'",
    "start:test": "prisma migrate deploy && NODE_ENV=production node server.js",
    "start:prod": "prisma migrate deploy && NODE_ENV=production node server.js",
    "typecheck": "tsc --noEmit",
    "prisma:generate": "prisma generate"
  },
